{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/controllers/*.ts"],
  "spec": {
		"securityDefinitions": {
			"api_key": {
				"type": "apiKey",
				"name": "X-Access-Token",
				"in": "header"
			},
      "base_url": {
        "type": "apiKey",
        "name": "base_url",
        "in": "path"
      }
		},
    "outputDirectory": "src",
    "specVersion": 3,
    "basePath": ["localhost:3333", "subsfy-development.up.railway.app"]
  },
  "routes": {
    "routesDir": "build"
  }
}
